{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
        "typeRoots": ["./node_modules/@types", "./types"],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts","types/**/*", "**/*.ts", "**/*.tsx", "src/tailwind.config.js", "src/components/content/ServiceContent.js", "src/components/Sidebar.js", "src/pages/_document.jsx", "src/pages/index.jsx", "src/pages/_app.jsx"],
  "exclude": ["node_modules"]
}
